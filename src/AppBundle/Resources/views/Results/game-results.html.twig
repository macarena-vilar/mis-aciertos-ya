<div class="row">
	<div class="col-xs-10">
		<table class="table-striped" style="width:100%">
			<tr>
				<td >
					<h3>
						<strong>Sorteos del d&iacute;a {{ gameDate }}</strong>
					</h3>
				</td>
			</tr>
			{% for r in rowList %}
			<tr class="vcenter">
				<td>
					<span class='entryNumberText'> 
					<h4><strong>Sorteo Nro. {{ r.drawNr }}</strong></h4> 
					</span>
				</td>
				<td>
					<div class="center-block" style="width:{{ nrWidth }}px;">
					{% for gH in r.gameHits["nrList"] %}
						<span class='entryNumber entryNumber{{ gH["hit"]== 0 ? 'No' }}Hit'>
					 		{{gH["nr"] }} 
						</span>
					{% endfor %}
					</div>
				</td>
				<td>
					<span class='entryNumberText'> 
						<h4><strong>{{ r.gameHits["prize"] }}</strong></h4> 
					</span>
				</td>
			</tr>
			{% endfor %}
		</table>
	</div>
</div>
